{
  "name": "vscode-bash",
  "displayName": "Bash",
  "description": "Full language support for the Bourne Again SHell (Bash)",
  "version": "0.1.0",
  "author": "Marcus R. Brown <me@igetgam.es>",
  "publisher": "igetgames",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/igetgames/vscode-bash"
  },
  "bugs": {
    "url": "https://github.com/igetgames/vscode-bash/issues"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:shellscript"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^12.19.3",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "mocha": "^8.2.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5",
    "vscode-test": "^1.4.1"
  }
}
